@model ServiceItem
@{
    ViewBag.Title = Model.MetaTitle;
    ViewBag.Description = Model.MetaDescription;
    ViewBag.Keywords = Model.MetaKeywords;
}
<div id="main">
    <article class="post">
        <h2>
            @Model.Title
        </h2>
        <h3>
            @Model.Subtitle
        </h3>
        <div>
            <img class="left" src="~/images/@Model.TitleImagePath" />
        </div>
    </article>
    <article class="post">
        @Html.Raw(Model.Text)
    </article>
</div>
    